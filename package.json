{
  "name": "reach-ssd-int-dashboard",
  "description": "REACH South Sudan INT Dashboard",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Maxym Malynowsky <maxym.malynowsky@reach-initiative.org>",
  "scripts": {
    "build": "rm -rf .cache && rm -rf public && gatsby build",
    "deploy": "yarn build && netlify deploy",
    "git": "yarn build && git push && git checkout master && git merge develop && git push && git checkout develop",
    "start": "gatsby develop",
    "network": "gatsby develop -H 0.0.0.0",
    "serve": "yarn build && gatsby serve",
    "format": "prettier --write 'src/**/*.{js,ts,jsx,tsx}'",
    "lint": "tsc && eslint --fix 'src/**/*.{js,ts,jsx,tsx}'",
    "upgrade-all": "./upgrade-all.sh && yarn upgrade"
  },
  "dependencies": {
    "bulma": "^0.7.5",
    "gatsby": "^2.15.34",
    "gatsby-image": "^2.2.25",
    "gatsby-plugin-catch-links": "^2.1.13",
    "gatsby-plugin-google-analytics": "^2.1.21",
    "gatsby-plugin-manifest": "^2.2.21",
    "gatsby-plugin-netlify": "^2.1.19",
    "gatsby-plugin-offline": "^3.0.13",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-sass": "^2.1.18",
    "gatsby-plugin-sharp": "^2.2.29",
    "gatsby-plugin-typescript": "^2.1.12",
    "gatsby-source-filesystem": "^2.1.31",
    "gatsby-transformer-csv": "^2.1.12",
    "gatsby-transformer-remark": "^2.6.28",
    "gatsby-transformer-sharp": "^2.2.21",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "1.x",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.4",
    "webpack": "^4.41.0"
  },
  "keywords": [
    "gatsby"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/impact-initiatives/reach-ssd-int-dashboard"
  }
}
